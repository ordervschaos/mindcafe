{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "moduleResolution": "node"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/_app.js",
    "components/RaveView.js",
    "components/PublishModal.js",
    "components/CreateRave.js"
, "components/cafe/CafeMealCard.jsx", "components/cafe/CafeMealView.jsx", "pages/cafe/index.jsx", "components/layouts/Layout.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
